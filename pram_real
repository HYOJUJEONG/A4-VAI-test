param set-default CA_ROTOR1_KM -0.05

param set-default CA_ROTOR2_PX 0.25
param set-default CA_ROTOR2_PY -0.495
param set-default CA_ROTOR2_KM 0.05

param set-default CA_ROTOR3_PX -0.52
param set-default CA_ROTOR3_PY -0.195
param set-default CA_ROTOR3_KM 0.05

param set-default CA_ROTOR4_PX 0.52
param set-default CA_ROTOR4_PY 0.195
param set-default CA_ROTOR4_KM 0.05

param set-default CA_ROTOR5_PX -0.25
param set-default CA_ROTOR5_PY 0.495
param set-default CA_ROTOR5_KM 0.05

param set-default CA_ROTOR6_PX 0.25
param set-default CA_ROTOR6_PY 0.495
param set-default CA_ROTOR6_KM -0.05

param set-default CA_ROTOR7_PX -0.25
param set-default CA_ROTOR7_PY -0.495
param set-default CA_ROTOR7_KM -0.05

param set-default PWM_MAIN_FUNC1 101
