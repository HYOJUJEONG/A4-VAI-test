param set-default MC_PITCHRATE_P 0.2
param set-default MC_PITCHRATE_I 0.0
param set-default MC_PITCHRATE_D 0.014
param set-default MC_PITCH_P 3.0

param set-default MC_YAWRATE_P 0.5
param set-default MC_YAWRATE_I 0.0
